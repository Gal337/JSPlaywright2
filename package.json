{
  "name": "jsplaywright2",
  "version": "1.0.0",
  "description": "Automated software testing with Playwright",
  "main": "index.js",
  "scripts": {
    "tests:chrome": "playwright test --config=playwright.config.js --project=Chromium",
    "tests:firefox": "playwright test --config=playwright.config.js --project=Firefox",
    "tests:webkit": "playwright test --config=playwright.config.js --project=Webkit",
    "tests:e2e": "playwright test --config=e2e.config.js --project=Chromium",
    "tests:visual:chrome": "playwright test --config=visual.config.js --project=Chromium",
    "tests:visual:firefox": "playwright test --config=visual.config.js --project=Firefox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gal337/JSPlaywright2.git"
  },
  "keywords": [
    "Playwright",
    "JavaScript",
    "automation",
    "testing"
  ],
  "author": "Gal Sesek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Gal337/JSPlaywright2/issues"
  },
  "homepage": "https://github.com/Gal337/JSPlaywright2#readme",
  "dependencies": {
    "@playwright/test": "^1.34.3",
    "prettier": "^2.8.8"
  }
}
